#include <stdio.h>
int main()
{
	int n;
	printf("thrchoices of menu are:\n\t1.pizza,rs239\n\t2.burger,rs129\n\t3.pasta,rs179\n\t4.french fries,ra99\n\t5.sandwich,rs149\n\n\nenter your choice:");
	scanf("%d",&n); 
	switch(n)
	{
		case 1:
			printf("food item-pizza\nprice-239");
			break;
		case 2:
			printf("food item-burger\nprice-129");
			break;
		case 3:
			printf("food item-pasta\nprice-179");
			break;
		case 4:
			printf("food item-french fries\nprice-99");
			break;
		case 5:
			printf("food item-sadnwhich\nprice-149");
			break;
		default:
			printf("food item is not on list please select from list");
	}
}
